{
  "name": "vueschool-forum",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint-script": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json,.htm,.html --ignore-path .gitignore",
    "lint-style": "stylelint '**/*.{css,scss,sass,vue}'",
    "lint-script-fix": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json,.htm,.html --ignore-path .gitignore --fix",
    "lint-style-fix": "stylelint ./**/*.{css,scss,sass,vue} --fix",
    "format": "prettier . --ignore-unknown --ignore-path .gitignore --check --write && npm run lint-script-fix && npm run lint-style-fix",
    "firebase-deploy": "firebase deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@hennge/vue3-pagination": "^1.0.17",
    "@vueuse/core": "^9.3.0",
    "@vueuse/integrations": "^9.3.0",
    "dayjs": "^1.11.5",
    "firebase": "^9.12.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "vee-validate": "^4.7.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/tsconfig": "^0.1.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "firestore-export-import": "^1.3.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-firestore-rules": "^0.1.5",
    "sass": "^1.55.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
